<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.safedrivev2.controllers.AdminLogInController">
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="250.0" />
        <AnchorPane layoutX="250.0" prefHeight="400.0" prefWidth="350.0">
            <children>
                <Label layoutX="58.0" layoutY="142.0" text="Username">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <TextField layoutX="55.0" layoutY="176.0" />
                <Label layoutX="58.0" layoutY="218.0" text="Password">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Button fx:id="cancelAdminButton" layoutX="250.0" layoutY="314.0" mnemonicParsing="false" onAction="#setAdminCancelButtonOnAction" text="Cancel" />
